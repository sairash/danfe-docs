#region max
function max(num1, num2) {

    if num1 > num2 {
        result = num1; 
    } else {
        result = num2;
    }

    return result; # variable defined and returned dynamically
}
#endregion max

# Calling Function
println("The maximum of the two numbers is ",max(10,4))
println("The maximum of the two numbers is ",max(5,6))